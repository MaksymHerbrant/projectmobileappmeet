<script type="text/javascript">
    $(document).ready(function(){

        $('#back-top a').click(function () {
            $('body,html').animate({
                scrollTop: 0
            }, 800);
            return false;
        });

        $(window).scroll(function(){

            var $w = $(this);

            if ($w.scrollTop() > 1200) $('#back-top').fadeIn();
            else  $('#back-top').fadeOut();

        });

    });
</script>


